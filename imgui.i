    const int wbuf_length = ImTextCountCharsFromUtf8(text, 0) + 1;
    HGLOBAL wbuf_handle = ::GlobalAlloc(0x0002, (SIZE_T)wbuf_length * sizeof(ImWchar));
    if (wbuf_handle == 0)
    {
